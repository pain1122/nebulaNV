{
  "name": "nebula-nv",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "check-types": "turbo run check-types",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,md,json}\"",
    "proto:gen": "node ./scripts/proto-gen.mjs",
    "proto:check": "node ./scripts/proto-gen.mjs --check",
    "prisma:gen": "pnpm -r -F @nebula/user-service -F @nebula/product-service -F @nebula/settings-service exec prisma generate -- --schema ./prisma/schema.prisma",
    "prisma:migrate:dev": "pnpm -r -F @nebula/user-service -F @nebula/product-service -F @nebula/settings-service exec prisma migrate dev -- --schema ./prisma/schema.prisma",
    "prisma:migrate:deploy": "pnpm -r -F @nebula/user-service -F @nebula/product-service -F @nebula/settings-service exec prisma migrate deploy -- --schema ./prisma/schema.prisma",
    "db:push": "pnpm -r -F @nebula/user-service -F @nebula/product-service -F @nebula/settings-service exec prisma db push -- --schema ./prisma/schema.prisma",
    "studio:user": "pnpm -C apps/user-service prisma studio --schema ./prisma/schema.prisma",
    "studio:product": "pnpm -C apps/product-service prisma studio --schema ./prisma/schema.prisma",
    "studio:settings": "pnpm -C apps/settings-service prisma studio --schema ./prisma/schema.prisma",
    "dev:auth": "pnpm -C apps/auth-service start:dev",
    "dev:user": "pnpm -C apps/user-service start:dev",
    "dev:product": "pnpm -C apps/product-service start:dev",
    "dev:settings": "pnpm -C apps/settings-service start:dev"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/bcrypt": "^6.0.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.5.2",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.36.0",
    "globals": "^16.4.0",
    "jest": "^30.2.0",
    "nx": "^21.5.3",
    "prettier": "^3.6.2",
    "prisma": "6.16.2",
    "proto": "^1.0.19",
    "protoc": "^32.1.0",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "ts-proto": "^2.7.7",
    "tsconfig-paths": "^4.2.0",
    "turbo": "^2.5.8",
    "typescript": "5.9.2"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.9.0",
    "@grpc/grpc-js": "^1.14.0",
    "@grpc/proto-loader": "^0.8.0",
    "@nebula/clients": "workspace:*",
    "@nebula/grpc-auth": "workspace:*",
    "@nebula/protos": "workspace:*",
    "@nestjs/common": "11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "11.1.6",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/microservices": "11.1.6",
    "@nestjs/platform-express": "11.1.6",
    "@prisma/client": "6.16.2",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "express": "^5.1.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "packageManager": "pnpm@10.17.1",
  "engines": {
    "node": ">=22"
  },
  "pnpm": {
    "overrides": {
      "@nestjs/common": "11.1.5",
      "@nestjs/core": "11.1.5",
      "@nestjs/microservices": "11.1.5",
      "@nestjs/platform-express": "11.1.5",
      "@nestjs/testing": "11.1.5",
      "prisma": "6.11.1",
      "@prisma/client": "6.11.1",
      "fstream": ">=1.0.13",
      "npmlog": "^7.0.1",
      "gauge": "^5.0.0",
      "are-we-there-yet": "^3.0.1",
      "inflight": "^2.0.0",
      "node-domexception": "^2.0.0",
      "glob": "^10.4.5",
      "rimraf": "^6.0.1"
    },
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "@swc/core",
      "bcrypt",
      "grpc-tools",
      "prisma",
      "protobufjs",
      "unrs-resolver"
    ],
    "ignoredBuiltDependencies": [
      "@nestjs/core",
      "nx"
    ]
  }
}