{
  "name": "@nebula/product-service",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prisma:gen": "prisma generate --schema ./prisma/schema.prisma",
    "prisma:migrate:dev": "prisma migrate dev --schema ./prisma/schema.prisma",
    "prisma:migrate:deploy": "prisma migrate deploy --schema ./prisma/schema.prisma",
    "db:push": "prisma db push --schema ./prisma/schema.prisma",
    "db:seed": "prisma db seed --schema ./prisma/schema.prisma",
    "db:studio": "prisma studio --schema ./prisma/schema.prisma",
    "postinstall": "pnpm run prisma:gen"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.6.2",
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.7.15",
    "@nestjs/common": "^11.1.5",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.5",
    "@nestjs/microservices": "^11.1.5",
    "@nestjs/platform-express": "^11.1.5",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "6.11.1",
    "compression": "^1.8.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.1.5",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.0",
    "@types/node": "^22.15.3",
    "jest": "^29.7.0",
    "prettier": "^3.6.0",
    "prisma": "6.11.1",
    "ts-node": "^10.9.2",
    "ts-proto": "^2.7.5",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2"
  }
}